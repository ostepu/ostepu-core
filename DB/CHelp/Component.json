{
    "name": "CHelp",
    "description": {
        "de": "die CHelp ist ein zentraler Sammelpunkt aller Hilfedateien"
    },
    "version": "1.0",
    "classFile": "CHelp.php",
    "className": "CHelp",
    "files": [
        {
            "path": ".htaccess"
        },
        {
            "path": "CHelp.php"
        },
        {
            "path": "index.php"
        },
        {
            "path": "Commands.json"
        },
        {
            "path": "info\/"
        },
        {
            "path": "help\/"
        }
    ],
    "links": [
        {
            "name": "request",
            "description": {
                "de": "über diesen Ausgang werden alle Hilfeseiten von anderen Komponenten angefragt"
            }
        },
        {
            "name": "getAlive",
            "description": {
                "de": "für den Befehl getAlive"
            }
        }
    ],
    "connector": [
        {
            "name": "request",
            "target": "CLocalObjectRequest",
            "description": {
                "de": "damit CHelp als lokales Objekt aufgerufen werden kann"
            }
        },
        {
            "name": "postPlatform",
            "target": "CInstall",
            "description": {
                "de": "der Installationsassistent soll uns bei der Plattforminstallation aufrufen"
            }
        },
        {
            "name": "getComponentProfiles",
            "target": "TApiConfiguration",
            "description": {
                "de": "damit unsere Aufrufe in die Standardprofile der CGate einsortiert werden"
            }
        },
        {
            "name": "getDescFiles",
            "target": "TDocuView",
            "description": {
                "de": "die Entwicklerdokumentation soll unsere Beschreibungsdatei nutzen"
            }
        }
    ]
}