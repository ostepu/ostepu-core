{
    "name": "CSystem",
    "description": {
        "de": "die CSystem bietet Anfragen an, welche eher allgemeine Aufgaben haben oder anderen Komponenten dienen sollen"
    },
    "version": "1.0",
    "classFile": "CSystem.php",
    "className": "CSystem",
    "files": [
        {
            "path": ".htaccess"
        },
        {
            "path": "CSystem.php"
        },
        {
            "path": "index.php"
        },
        {
            "path": "Commands.json"
        }
    ],
    "connector": [
        {
            "name": "request",
            "target": "CLocalObjectRequest",
            "description": {
                "de": "damit CSystem als lokales Objekt aufgerufen werden kann"
            }
        },
        {
            "name": "getComponentProfiles",
            "target": "TApiConfiguration",
            "description": {
                "de": "damit unsere Aufrufe in die Standardprofile der CGate einsortiert werden"
            }
        },
        {
            "name": "getDescFiles",
            "target": "TDocuView",
            "description": {
                "de": "die Entwicklerdokumentation soll unsere Beschreibungsdatei nutzen"
            }
        }
    ]
}