[
    {
        "path": "(\/:pre)\/course",
        "method": "post",
        "description": {
            "de": "installiert die Komponente in die Veranstaltung"
        }
    },
    {
        "path": "(\/:pre)\/course\/:courseid",
        "method": "delete",
        "description": {
            "de": "entfernt die Komponente aus der Veranstaltung"
        }
    },
    {
        "path": "(\/:pre)\/insert",
        "method": "post",
        "description": {
            "de": "fügt einen neuen Testfall ein"
        }
    },
    {
        "path": "(\/:pre)\/pop",
        "method": "get",
        "description": {
            "de": "sperrt den nächsten unbearbeiteten Testfall und liefert ihn zurück"
        }
    },
    {
        "path": "(\/:pre)\/testcase(\/testcase)\/:testcaseid",
        "method": "put",
        "description": {
            "de": "editiert einen Testfall"
        }
    },
    {
        "path": "(\/:pre)\/testcase\/submission\/:sid\/course\/:cid",
        "method": "get",
        "description": {
            "de": "ermittelt einen Testfall zu einer Einsendung"
        }
    }
]