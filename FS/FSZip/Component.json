{
    "name": "FSZip",
    "description": {
        "de": "Die FSZip erstellt ZIP-Archive. Diese k\u00f6nnen tempor\u00e4r oder dauerhaft im Dateisystem hinterlegt werden."
    },
    "version": "1.0",
    "classFile": "FSZip.php",
    "className": "FSZip",
    "files": [
        {
            "path": ".htaccess"
        },
        {
            "path": "FSZip.php"
        },
        {
            "path": "Commands.json"
        },
        {
            "path": "index.php"
        }
    ],
    "connector": [
        {
            "name": "request",
            "target": "CLocalObjectRequest",
            "description": {
                "de": "damit FSZip als lokales Objekt aufgerufen werden kann"
            }
        },
        {
            "name": "postPlatform",
            "target": "CInstall",
            "description": {
                "de": "der Installationsassistent soll uns bei der Plattforminstallation aufrufen"
            }
        },
        {
            "name": "getComponentProfiles",
            "target": "TApiConfiguration",
            "description": {
                "de": "damit unsere Aufrufe in die Standardprofile der CGate einsortiert werden"
            }
        }
    ]
}