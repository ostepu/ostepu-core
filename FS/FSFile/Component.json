{
    "name": "FSFile",
    "description": {
        "de": "Die FSFile speichert allgemeine Dateien, welche beispielsweise hochgeladen oder zwischenzeitlich erzeugt wurden."
    },
    "version": "1.0",
    "classFile": "FSFile.php",
    "className": "FSFile",
    "files": [
        {
            "path": ".htaccess"
        },
        {
            "path": "FSFile.php"
        },
        {
            "path": "Commands.json"
        },
        {
            "path": "index.php"
        }
    ],
    "connector": [
        {
            "name": "request",
            "target": "CLocalObjectRequest",
            "description": {
                "de": "damit FSFile als lokales Objekt aufgerufen werden kann"
            }
        },
        {
            "name": "postPlatform",
            "target": "CInstall",
            "description": {
                "de": "der Installationsassistent soll uns bei der Plattforminstallation aufrufen"
            }
        },
        {
            "name": "getComponentProfiles",
            "target": "TApiConfiguration",
            "description": {
                "de": "damit unsere Aufrufe in die Standardprofile der CGate einsortiert werden"
            }
        },
        {
            "name": "getDescFiles",
            "target": "TDocuView",
            "description": {
                "de": "die Entwicklerdokumentation soll unsere Beschreibungsdatei nutzen"
            }
        }
    ]
}