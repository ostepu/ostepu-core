{
    "name": "FSPdf",
    "description": {
        "de": "Die FSPdf erzeugt PDF's. Dabei kann man mittels HTML und CSS die Ausgabe formatieren."
    },
    "version": "1.0",
    "classFile": "FSPdf.php",
    "className": "FSPdf",
    "files": [
        {
            "path": ".htaccess"
        },
        {
            "path": "FSPdf.php"
        },
        {
            "path": "Commands.json"
        },
        {
            "path": "_tcpdf_6.0.095\/"
        },
        {
            "path": "index.php"
        }
    ],
    "connector": [
        {
            "name": "request",
            "target": "CLocalObjectRequest",
            "description": {
                "de": "damit FSPdf als lokales Objekt aufgerufen werden kann"
            }
        },
        {
            "name": "postPlatform",
            "target": "CInstall",
            "description": {
                "de": "der Installationsassistent soll uns bei der Plattforminstallation aufrufen"
            }
        },
        {
            "name": "getComponentProfiles",
            "target": "TApiConfiguration",
            "description": {
                "de": "damit unsere Aufrufe in die Standardprofile der CGate einsortiert werden"
            }
        },
        {
            "name": "getDescFiles",
            "target": "TDocuView",
            "description": {
                "de": "die Entwicklerdokumentation soll unsere Beschreibungsdatei nutzen"
            }
        }
    ]
}