{
    "name": "LMarking",
    "description": {
        "de": "Diese Komponente ermöglicht das einfache Speichern von Korrekturobjekten. Dabei prüft sie selbständig, ob die Korrekturdatei in der Plattform hinterlegt werden muss und ob es sich um eine Änderung oder eine Neuerstellung des Korrekturobjekts handelt."
    },
    "version": "1.0",
    "classFile": "LMarking.php",
    "className": "LMarking",
    "files": [
        {
            "path": ".htaccess"
        },
        {
            "path": "LMarking.php"
        },
        {
            "path": "index.php"
        },
        {
            "path": "Commands.json"
        }
    ],
    "links": [
        {
            "name": "file",
            "target": "LFile",
            "description": {
                "de": "zum Speichern von Dateien"
            },
            "links": [
                {
                    "method": "POST",
                    "path": "\/file"
                }
            ]
        },
        {
            "name": "marking",
            "target": "DBMarking",
            "description": {
                "de": "zum Speichern, Ändern und Löschen von Korrekturen"
            },
            "links": [
                {
                    "method": "DELETE",
                    "path": "\/marking\/marking\/:markingid"
                },
                {
                    "method": "POST",
                    "path": "\/marking"
                },
                {
                    "method": "PUT",
                    "path": "\/marking\/marking\/:markingid"
                }
            ]
        }
    ],
    "connector": [
        {
            "name": "request",
            "target": "CLocalObjectRequest",
            "description": {
                "de": "damit LMarking als lokales Objekt aufgerufen werden kann"
            }
        },
        {
            "name": "getDescFiles",
            "target": "TDocuView",
            "description": {
                "de": "die Entwicklerdokumentation soll unsere Beschreibungsdatei nutzen"
            }
        }
    ]
}