{
    "name": "LProcessor",
    "description": {
        "de": "Über diese Komponente können Einsendungen im System hinterlegt werden. Sie speichert die Einsendung, Einsendungsdatei und führt nachgeschaltene Verarbeitungen aus. Die Verarbeitungen müssen für die jeweilige Aufgabe definiert sein."
    },
    "version": "1.0",
    "classFile": "LProcessor.php",
    "className": "LProcessor",
    "files": [
        {
            "path": ".htaccess"
        },
        {
            "path": "LProcessor.php"
        },
        {
            "path": "index.php"
        },
        {
            "path": "Commands.json"
        },
        {
            "path": "info\/"
        },
        {
            "path": "messages\/"
        }
    ],
    "links": [
        {
            "name": "submission",
            "target": "LSubmission",
            "description": {
                "de": "zum Speichern der Einsendung und Einsendungsdatei"
            },
            "links": [
                {
                    "method": "POST",
                    "path": "\/submission"
                }
            ]
        },
        {
            "name": "marking",
            "target": "LMarking",
            "description": {
                "de": "zum Speichern der Korrekturen, falls die Verarbeitungen eine Korrektur erzeugt haben"
            },
            "links": [
                {
                    "method": "POST",
                    "path": "\/marking"
                }
            ]
        },
        {
            "name": "processorDb",
            "target": "DBProcess",
            "description": {
                "de": "Zum Abrufen der Verarbeitungen für diese Aufgabe. Zudem wird dieser Ausgang genutzt, um neue Verarbeitungen zu speichern und zu ändern."
            },
            "links": [
                {
                    "method": "POST",
                    "path": "\/process"
                },
                {
                    "method": "PUT",
                    "path": "\/process\/process\/:processid"
                },
                {
                    "method": "GET",
                    "path": "\/process\/exercise\/:exerciseid"
                }
            ]
        },
        {
            "name": "attachment",
            "target": "DBProcessAttachment",
            "description": {
                "de": "zum Speichern der Verarbeitunsanhänge"
            },
            "links": [
                {
                    "method": "POST",
                    "path": "\/attachment"
                }
            ]
        },
        {
            "name": "workFiles",
            "target": "DBProcessWorkFiles",
            "description": {
                "de": "zum Speichern der Arbeitsdaten einer Verarbeitung"
            },
            "links": [
                {
                    "method": "POST",
                    "path": "\/attachment"
                }
            ]
        },
        {
            "name": "getExerciseExerciseFileType",
            "target": "DBExerciseFileType",
            "description": {
                "de": "zum Abrufen der erlaubten Dateitypen einer Aufgabe"
            },
            "links": [
                {
                    "method": "GET",
                    "path": "\/exercisefiletype\/exercise\/:eid"
                }
            ]
        },
        {
            "name": "file",
            "target": "LFile",
            "description": {
                "de": "zum Speichern von Dateien"
            },
            "links": [
                {
                    "method": "POST",
                    "path": "\/file"
                }
            ]
        },
        {
            "name": "postCourse",
            "target": "DBProcess",
            "description": {
                "de": "um nachgeschaltene Komponenten in die Veranstaltung installieren zu können"
            },
            "links": [
                {
                    "method": "GET",
                    "path": "\/link\/exists\/course\/:courseid"
                },
                {
                    "method": "POST",
                    "path": "\/course"
                },
                {
                    "method": "DELETE",
                    "path": "\/course\/course\/:courseid"
                }
            ]
        }
    ],
    "connector": [
        {
            "name": "deleteCourse",
            "target": "LCourse",
            "description": {
                "de": "wenn eine Veranstaltung gelöscht wird, dann müssen auch unsere Tabellen entfernt werden"
            },
            "priority": 150
        },
        {
            "name": "postCourse",
            "target": "LCourse",
            "description": {
                "de": "wenn eine neue Veranstaltung angelegt wird, dann wollen wir auch aufgerufen werden"
            },
            "priority": 150
        },
        {
            "name": "request",
            "target": "CLocalObjectRequest",
            "description": {
                "de": "damit LProcessor als lokales Objekt aufgerufen werden kann"
            }
        }
    ]
}