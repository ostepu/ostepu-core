{
    "name": "LExercise",
    "description": {
        "de": "Die LExercise bietet Anfragen für Übungsaufgaben an. Dabei enthält sie zunächst nur einen Befehl zum Speichern neuer Übungsaufgaben."
    },
    "version": "1.0",
    "classFile": "LExercise.php",
    "className": "LExercise",
    "files": [
        {
            "path": ".htaccess"
        },
        {
            "path": "LExercise.php"
        },
        {
            "path": "index.php"
        },
        {
            "path": "Commands.json"
        }
    ],
    "links": [
        {
            "name": "controller",
            "target": "LController",
            "description": {
                "de": "über diesen Ausgang werden diverse Anfragen behandelt"
            },
            "links": [
                {
                    "method": "POST",
                    "path": "\/FS\/file"
                },
                {
                    "method": "POST",
                    "path": "\/DB\/file"
                },
                {
                    "method": "GET",
                    "path": "\/hash\/:hash"
                },
                {
                    "method": "POST",
                    "path": "\/DB\/exercise"
                },
                {
                    "method": "POST",
                    "path": "\/DB\/exercisefiletype"
                },
                {
                    "method": "GET",
                    "path": "\/DB\/exercise\/:exerciseid"
                },
                {
                    "method": "DELETE",
                    "path": "\/DB\/exercise\/exercise\/:$exerciseid"
                },
                {
                    "method": "PUT",
                    "path": "\/DB\/exercise\/:exerciseid"
                }
            ]
        },
        {
            "name": "postAttachment",
            "target": "LAttachment",
            "description": {
                "de": "zum Speichern von Übungsanhängen"
            },
            "links": [
                {
                    "method": "POST",
                    "path": "\/attachment"
                }
            ]
        }
    ]
}