{
    "name": "LCourse",
    "description": {
        "de": "Die LCourse bietet Anfragen zum Anlegen einer neuen Veranstaltung an."
    },
    "version": "1.0",
    "classFile": "LCourse.php",
    "className": "LCourse",
    "files": [
        {
            "path": ".htaccess"
        },
        {
            "path": "LCourse.php"
        },
        {
            "path": "index.php"
        },
        {
            "path": "Commands.json"
        }
    ],
    "links": [
        {
            "name": "controller",
            "target": "LController",
            "description": {
                "de": "Ãœber diesen Ausgang werden verschiedene Anfragen an Datenbankkomponenten gestellt. Es muss sich um eine spezielle Verteilerkomponente handeln (normalerweise LController)."
            },
            "links": [
                {
                    "method": "PUT",
                    "path": "\/DB\/course\/course\/:courseid"
                },
                {
                    "method": "POST",
                    "path": "\/DB\/coursestatus"
                },
                {
                    "method": "GET",
                    "path": "\/DB\/user\/course\/:courseid"
                },
                {
                    "method": "GET",
                    "path": "\/DB\/course\/user\/:userid"
                }
            ]
        },
        {
            "name": "postCourse",
            "target": [
                "DBCourse"
            ],
            "description": {
                "de": "zum Anlegen einer Veranstaltung (in postCourse)"
            },
            "links": [
                {
                    "method": "POST",
                    "path": "\/course"
                }
            ]
        },
        {
            "name": "deleteCourse",
            "target": [
                "DBCourse"
            ],
            "description": {
                "de": "zum Entfernen einer Veranstaltung"
            },
            "links": [
                {
                    "method": "DELETE",
                    "path": "\/course\/:courseid"
                }
            ]
        }
    ],
    "connector": [
        {
            "name": "request",
            "target": "CLocalObjectRequest",
            "description": {
                "de": "damit LCourse als lokales Objekt aufgerufen werden kann"
            }
        }
    ]
}